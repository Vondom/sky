<?xml version="1.0" encoding="UTF-8"?>
<jrat>
	<settings>
		<property name="httpServerEnabled" value="true"/>
	</settings>

	<profile name="my code">

		<criteria>
			<include className="com.sky.sample.*" methodName="*" signature="*"/>
		</criteria>

		<handlers>

			<handler factory="com.sky.profiler.jrat.provider.RealTimeMethodHandlerFactory">
				<property name="projectId" value="${projectId}" />
				<property name="workId" value="${workId}"
			</handler>

		</handlers>

	</profile>
</jrat>